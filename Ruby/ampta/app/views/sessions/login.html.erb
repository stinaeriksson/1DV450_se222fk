<h1>Sessions#login,</h1>
<p>Find me in app/views/sessions/login,.html.erb</p>

<h1> Välkommen</h1>


<% @page_title = "UserAuth | Login" -%>

<div class= "Login_Form">
	<h2>Logga in</h2>
	<%= form_tag(:action => 'login_attempt') do %>
		<p>Email:</br> <%= text_field_tag(:email) %></p> 
		<p>Lösenord:</br> <%= password_field_tag :password %></p> 
		<%= submit_tag("Logga in") %>
	<% end %>
</div>